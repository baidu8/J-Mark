{
  "manifest_version": 3,
  "name": "书签数据导出工具",
  "version": "1.0.0",
  "description": "由江大爷开发的一款极简、纯净的书签导出工具，支持搜索过滤和层级保留。",
  "permissions": ["bookmarks"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "exporter.html"
  }
}